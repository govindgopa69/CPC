#include <iostream>
using namespace std;

int main() {
	long long int t, n, d;
	cin>>t;
	for(int i=0; i<t; i++){
	    cin>>n;
	    cin>>d;
	    long long int arr[n];
	    for(int j=0; j<n; j++){
	        cin>>arr[j];
	    }
	    for(int j=d; j<n; j++){
	        cout<<arr[j]<<" ";
	    }
	    for(int j=0; j<d; j++){
	        cout<<arr[j]<<" ";
	    }
	    cout<<"\n";
	}
	return 0;
}
